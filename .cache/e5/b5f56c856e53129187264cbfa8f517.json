n":0},"generated":{"line":354,"column":0}}],"sources":{"css/style.css":"\r\n/* NAVIGATION */\r\n.logo-text,.secondary-logo-text{\r\n    font-size: 2.4rem;\r\n    font-weight: 600;\r\n    padding-left: 4.8rem;\r\n}\r\n.logo-text.hide{\r\n    display: none;\r\n}\r\n.secondary-logo-text.hide{\r\n    display: none;\r\n}\r\n\r\n.section-nav {\r\n   \r\n    border-radius: 5px;\r\n    height: 6rem;\r\n    background-color: #eee;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n   \r\n}\r\n.section-search-bar {\r\n    display: flex;\r\n    justify-content: center;\r\n    padding: 1.2rem 0;\r\n}\r\n.section-search-bar.hide{\r\n    display: none;\r\n}\r\n.cart-container{\r\n    display: flex;\r\n    flex-direction:row;\r\n    align-items: center;\r\n    padding-right: 4.8rem;\r\n    gap:2.4rem;\r\n}\r\n.link:link,\r\n.link:visited{\r\n    font-size: 2.4rem;\r\n    font-weight: 600;\r\n    text-decoration: none;\r\n   \r\n    color: #343a40;\r\n   \r\n}\r\n.link.hide{\r\n    display: none;\r\n}\r\n.active{\r\nborder-bottom: 1px solid #343a40;\r\n}\r\n.cart-icon-container{\r\n        width: 6rem;\r\n        height: 6rem;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        text-decoration: none;\r\n        border-radius: 4px;\r\n        background-color: #ccc;\r\n}\r\n.cart-icon-container.hide{\r\n    display: none;\r\n}\r\n.cart-quantity{\r\n    color: #343a40;\r\n    align-self: flex-start;\r\n    font-weight: 700;\r\n    font-size: 1.6rem;\r\n}\r\n.cart-icon {\r\n    color:#343a40;\r\n    height: 3.6rem;\r\n    width:3.6rem;\r\n}\r\n.input{\r\n    display: flex;\r\n    gap:0.2rem;\r\n    padding-left: 30rem;\r\n    padding-top: 2.4rem;\r\n}\r\n\r\n.search-input{\r\n    width: 35rem;\r\n   border-top: none;\r\n   border-right: none;\r\n   border-left: none;\r\n   outline:none;\r\n}\r\n.search-icon-container ,.filter-icon-container {\r\n    display: inline-block;\r\n    padding: 0.6rem 1.2rem;\r\n    border-radius: 5px;\r\n    background-color: #777;\r\n    cursor: pointer;\r\n}\r\n.filter-icon-container{\r\n    display: none;\r\n}\r\n.icon-search {\r\n    color:#fff;\r\n    height: 1.8rem;\r\n    width: 1.8rem;\r\n   \r\n}\r\n/* Products */\r\n.section-products{\r\npadding-top: 1.2rem;\r\n}\r\n.section-products-catalog{\r\n    grid-column: 2/-1;\r\n}\r\n\r\n.product-card{\r\n    width: 80%;\r\n  padding: 1.2rem 1.2rem 1.2rem 1.2rem;\r\n  box-shadow: 0 1rem 2rem rgba(0,0,0,0.09);\r\n  align-self: center;\r\n  justify-self: center;\r\n  \r\n} \r\n.product{\r\n    /* background-color: #e9ecef; */\r\n    padding-top: 0.5rem;\r\n}\r\n.product-name, .product-price{\r\n    font-weight: 700;\r\n    font-size: 1.6rem;\r\n}\r\n.product-name{\r\n    padding-left: 1rem;\r\n    line-height: 1.8;\r\n}\r\n\r\n.add--btn{\r\n    display: flex;\r\n   \r\n    width: 11rem;\r\n    height: 3.4rem;\r\n    color:#fff;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background-color: #495057;\r\n    border: none;\r\n  \r\n    cursor: pointer;\r\n}\r\n.primary--content{\r\n    font-weight: 600;\r\n    width:100%;\r\n    height: 100%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.content .primary--content{\r\n    display: none;\r\n}\r\n\r\n.secondary--content{\r\n    display: grid;\r\n    grid-template-columns: repeat(3,1fr);\r\n    width: 100%;\r\n    height: 4rem;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.filter--icon{\r\n    display: none;\r\n    color: #fff;\r\n    height: 1.8rem;\r\n    width: 1.8rem;\r\n}\r\n.plus--icon{\r\n    height: 3rem;\r\n    width: 100%;\r\n    padding: 0.8rem 0.8rem;\r\n    stroke: #fff;\r\n    cursor: pointer;\r\n    justify-self: center;\r\n    \r\n}\r\n.minus--icon{\r\nheight: 3rem;\r\nwidth: 100%;\r\npadding: 0.8rem 0.8rem;\r\nstroke: #fff;\r\ncursor: pointer;\r\njustify-self: center;\r\n\r\n}\r\n.minus--icon:hover{\r\n  color: green;\r\n   \r\n}\r\n.btn-cart--quantity{\r\n    font-weight: 600;\r\n}\r\n.product-image{\r\n    width: 100%;\r\n    height: 10rem;\r\n  \r\n}\r\n.product-description{\r\n    padding-top: 1.2rem;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-evenly;\r\n}\r\n/* Forms */\r\n.section-filter{\r\n    position: fixed;\r\n    top:14.2rem;\r\n    left: 20rem;\r\n    width: 25rem;\r\n    justify-self: center;\r\n    align-self: center;\r\n    box-shadow: 0 1rem 2rem rgba(0, 0, 0, 0.09);\r\n    display: grid;\r\n    grid-template-columns: 85fr 15fr;\r\n    padding:1.2rem 3.6rem;\r\n    gap:1.2rem;\r\n}\r\n.close-btn--container{\r\n    display: none;\r\n}\r\n\r\n.form-container{\r\n    grid-column: 1/2;\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap:0.8rem;\r\n}\r\n.filter-heading{\r\n    font-weight: 700;\r\n} \r\n\r\n.form-filter label{\r\n    font-weight: 600;\r\n line-height: 1.8;\r\n}\r\n\r\n/* cart pre-checkout-styles */\r\n.section-cart--precheckout {\r\n   display: flex;\r\n   flex-direction: column;\r\n   gap:2.4rem;\r\n}\r\n.section-cart--precheckout.hide{\r\n    display: none;\r\n}\r\n.cart-precheckout--container{\r\n    width: 40rem;\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap:2.4rem;\r\n} \r\n.cart-precheckout--heading{\r\n    display: none;\r\n    padding: 2.4rem 0 2.4rem 5.2rem;\r\n    font-size: 2.4rem;\r\n    font-weight: 500;\r\n}\r\n\r\n.cart-precheckout {\r\ndisplay: grid;\r\ngrid-template-columns: 1fr 2fr 2fr\r\n}\r\n.cart-precheckout--image{\r\n    width: 5rem;\r\n}\r\n.cartprechekout-btn--container{\r\n    \r\n    align-self: center;\r\n    display: flex;\r\n    gap:1.2rem;\r\n}\r\n.cart-checkout--btn {\r\n  font-weight: 600;\r\n  height: 3.2rem;\r\n  width: 6.2rem;\r\n  padding: 0 1.2rem;\r\n  border-radius: 4px;\r\n}\r\n.cartprecheckout--description{\r\n    \r\n    align-self: center;\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap:0.6rem;\r\n}\r\n.cartprecheckout--name{\r\n    font-weight: 700;\r\n    font-size: 1.6rem;\r\n   \r\n} \r\n.cartprecheckout--price{\r\n    font-size: 1.2rem;\r\n    font-weight: 800;\r\n}\r\n.arrow-up{\r\n    height: 1.6rem;\r\n    width: 1.6rem;\r\n}\r\n.cart-checkout--btn {\r\n    width: 10rem;\r\n    display: flex;\r\n    gap:0.5rem;\r\n    align-items: center;\r\n    justify-content: center;\r\n    cursor: pointer;\r\n    \r\n}\r\n.add{\r\n    border: 2px solid rgb(157, 157, 157);\r\n    background-color: #ccc;\r\n}\r\n.delete{\r\n    border: 2px solid #777;\r\n}\r\n.price-total{\r\n    border-top:1px solid #333;\r\n    display: grid;\r\n    grid-template-columns: 0.2fr 0.4fr 0.8fr;\r\n    height: 3.2rem;\r\n}\r\n\r\n.total-price--heading,.total-amount{\r\n    font-weight: 700;\r\n    font-size: 1.6rem;\r\n    align-self: center;\r\n}\r\n.total-amount{\r\n    grid-column: 2/3;\r\n   \r\n}\r\n\r\n/* error */\r\n.noresults--message{\r\n    grid-column: 2/-1;\r\n    font-size: 2.4rem;\r\n    color: red;\r\n  \r\n}\r\n/* cart error message */\r\n.errorMessage{\r\n    color: red;\r\n    font-size: 1.4rem;\r\n    padding: 1rem 0 0 2rem;\r\n}"},"lineCount":354}},"error":null,"hash":"b42c7ae917348e116df63b952ecd292f","cacheData":{"env":{}}}